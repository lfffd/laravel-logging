{
  "name": "lfffd/laravel-logging",
  "description": "Advanced Laravel logging engine with structured correlation, PII guard, and performance diagnostics",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "lfffd",
      "homepage": "https://github.com/lfffd"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0|^11.0",
    "monolog/monolog": "^3.0",
    "ramsey/uuid": "^4.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "laravel/pint": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Superlog\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Superlog\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Superlog\\SuperlogServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}